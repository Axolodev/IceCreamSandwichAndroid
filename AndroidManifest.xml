<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfitnessapplication"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Videos"
            android:label="@string/title_activity_videos" >
        </activity>
        <activity
            android:name=".Configuracion"
            android:label="@string/title_activity_configuracion" >
        </activity>
        <activity
            android:name=".Historial"
            android:label="@string/title_activity_historial" >
        </activity>
        <activity
            android:name=".Cronometro"
            android:label="@string/title_activity_cronometro" >
        </activity>
        <activity
            android:name=".Medicion"
            android:label="@string/title_activity_medicion" >
        </activity>
        <activity
            android:name=".Playlist"
            android:label="@string/title_activity_playlist" >
        </activity>
        <activity
            android:name=".Resultado"
            android:label="@string/title_activity_resultado" >
        </activity>
    </application>

</manifest>
